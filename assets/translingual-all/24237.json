                {
                  "level": 4,
                  "title": "Pronunciation",
                  "text": "{{ja-pron|yomi=k|ばら|acc=1|acc_ref=DJR}}"
                },
                {
                  "level": 4,
                  "title": "Noun",
                  "text": "{{ja-noun|ばら|バラ}}\n\n# short for {{l|ja|肋肉|tr=''[[baraniku]]''}}: [[rib]] {{qualifier|meat, generally [[beef]] or [[pork]]}}",
                  "subSections": [
                    {
                      "level": 5,
