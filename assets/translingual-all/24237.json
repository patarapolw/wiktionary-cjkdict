                {
                  "level": 4,
                  "title": "Noun",
                  "text": "{{ja-noun|あばら}}\n\n# short for {{l|ja|肋骨|tr=''[[abarabone]]''}}: [[rib]] {{qualifier|body part}}"
                }
              ]
            },
            {
              "level": 3,
              "title": "Etymology 2",
              "text": "{{ja-kanjitab|ばら|yomi=k}}\nShortened from {{l|ja|あばら|tr=''abara''}}.",
